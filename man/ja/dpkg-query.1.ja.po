"    B<MSDOS-Filename>\n"
"    B<Maintainer>\n"
"    B<Origin>\n"
"    B<Package>\n"
"    B<Pre-Depends>\n"
"    B<Priority>\n"
"    B<Provides>\n"
"    B<Recommends>\n"
"    B<Replaces>\n"
"    B<Revision>\n"
"    B<Section>\n"
"    B<Size>\n"
"    B<Source>\n"
"    B<Status>\n"
"    B<Suggests>\n"
"    B<Version>\n"
msgstr ""

