VPATH		= @srcdir@
srcdir		= @srcdir@
top_srcdir	= @top_srcdir@

default: all
include ../../Makefile.conf

MAN5PAGES	= deb-control.5 deb-old.5 deb.5 dpkg.cfg.5 dselect.cfg.5

all:

clean: 

distclean: clean 
	$(RM) Makefile

install: install-doc

install-doc:
	$(mkinstalldirs) $(DESTDIR)$(man5dir) 
	set -e ; for i in $(MAN5PAGES) ; do \
		if test -f $$i ; then d= ; else d="$(srcdir)/" ; fi ; \
		$(INSTALL_DATA) $$d$$i $(DESTDIR)$(man5dir) ; \
	done

.PHONY: all clean distclean install install-doc
