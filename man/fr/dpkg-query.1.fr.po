"    B<Enhances>\n"
"    B<Essential>\n"
"    B<Filename>\n"
"    B<Installed-Size>\n"
"    B<MD5sum>\n"
"    B<MSDOS-Filename>\n"
"    B<Maintainer>\n"
"    B<Origin>\n"
"    B<Package>\n"
"    B<Pre-Depends>\n"
"    B<Priority>\n"
"    B<Provides>\n"
"    B<Recommends>\n"
"    B<Replaces>\n"
"    B<Revision>\n"
"    B<Section>\n"
"    B<Size>\n"
"    B<Source>\n"
"    B<Status>\n"
"    B<Suggests>\n"
"    B<Version>\n"
msgstr ""
"    B<Architecture>\n"
"    B<Bugs>\n"
"    B<Conffiles>\n"
"    B<Config-Version>\n"
"    B<Conflicts>\n"
"    B<Depends>\n"
