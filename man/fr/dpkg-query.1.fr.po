"    B<Enhances>\n"
"    B<Essential>\n"
"    B<Filename>\n"
"    B<Installed-Size>\n"
"    B<MD5sum>\n"
"    B<MSDOS-Filename>\n"
"    B<Maintainer>\n"
"    B<Origin>\n"
"    B<Package>\n"
"    B<Pre-Depends>\n"
"    B<Priority>\n"
"    B<Provides>\n"
