#: ../../man/dpkg.1:532
#, no-wrap
msgid "ENVIRONMENT VARIABLES"
msgstr ""

# type: TP
#: ../../man/dpkg.1:533
#, no-wrap
msgid "B<DPKG_NO_TSTP>"
msgstr ""

# type: Plain text
