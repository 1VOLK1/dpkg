#, no-wrap
msgid ""
"    B<-b>, B<--build>,\n"
"    B<-c>, B<--contents>,\n"
"    B<-I>, B<--info>,\n"
"    B<-f>, B<--field>,\n"
"    B<-e>, B<--control>,\n"
"    B<-x>, B<--extract>,\n"
"    B<-X>, B<--vextract>, and\n"
"    B<--fsys-tarfile>.\n"
msgstr ""
"    B<-b>, B<--build>,\n"
"    B<-c>, B<--contents>,\n"
"    B<-I>, B<--info>,\n"
"    B<-f>, B<--field>,\n"
"    B<-e>, B<--control>,\n"
"    B<-x>, B<--extract>,\n"
"    B<-X>, B<--vextract>, e\n"
"    B<--fsys-tarfile>.\n"

# type: Plain text
#: ../../man/dpkg.1:42
