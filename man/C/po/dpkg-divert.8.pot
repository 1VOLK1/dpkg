#, no-wrap
msgid "DPKG-DIVERT"
msgstr ""

# type: TH
#: dpkg-divert.8:1
#, no-wrap
msgid "December 1999"
msgstr ""

# type: TH
#: dpkg-divert.8:1
#, no-wrap
msgid "Debian Project"
msgstr ""

# type: TH
#: dpkg-divert.8:1
#, no-wrap
