#: dpkg.1:548
msgid "B< dpkg -l '*vi*'>"
msgstr ""

# type: Plain text
#: dpkg.1:551
msgid "To see the entries in I</var/lib/dpkg/available> on two packages:"
msgstr ""

# type: Plain text
#: dpkg.1:553
