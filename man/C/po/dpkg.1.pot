msgstr ""

# type: Plain text
#: dpkg.1:556
msgid "To search the listing of packages yourself:"
msgstr ""

# type: Plain text
#: dpkg.1:558
msgid "B< less /var/lib/dpkg/available>"
msgstr ""

# type: Plain text
#: dpkg.1:561
msgid "To remove an installed elvis package:"
msgstr ""

